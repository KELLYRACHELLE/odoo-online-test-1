<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <record model="ir.ui.view" id="view_order_form_inherit_academy">
            <field name="name">sale.order.form.inherit.academy</field>
            <field name="model">sale.orer</field>
            <field name="inherit-id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='payement_term_id']" position=after>
                    <field name="session_id"/>
               </xpath>
                    <field name="patner_id" position="before">
                        <field name="instructor_id"/>
            </field>
                    
                    <xpath expr="//sheet//notebook" position="inside">
                        <page sring="Students" attrs=>
                            <field name="student_ids">    
                                <tree>
                                    <field name="name"/>
                                    <field name="phone"/>  
                                </tree>
                            </field>
                        </page>
                    </xpath>
                </field>        
        </record>
    </data>
</odoo>